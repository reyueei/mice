<plugin name='zipped-resources' version='1.0.1' grailsVersion='1.3 &gt; *'>
  <author>Marc Palmer</author>
  <authorEmail>marc@grailsrocks.com</authorEmail>
  <title>Zipped Resources</title>
  <description>\
GZip encodes static resources using the Resources plugin framework
</description>
  <documentation>http://grails.org/plugin/zipped-resources</documentation>
  <type>ZippedResourcesGrailsPlugin</type>
  <resources>
    <resource>BootStrap</resource>
    <resource>org.grails.plugin.zippedresources.ZipResourceMapper</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins>
    <runtime>
      <plugin group='org.grails.plugins' name='resources' version='1.1.6' />
    </runtime>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>